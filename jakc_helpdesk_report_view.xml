                    </group>
                    <footer>
                        <button name="generate_report" string="Show" type="object" default_focus="1" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>                                
            </field>
        </record>
        
        <record id="action_helpdesk_monthly_summary_report_form" model="ir.actions.act_window">
            <field name="name">Helpdesk Monthly Summary Report</field>
            <field name="res_model">helpdesk.monthly.summary.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record> 
        
                
    </data>
</openerp>

	